EXEC sp_execute_external_script
@language = N'Python',
@script = N'
import pandas as pd

# Load the JSON into a DataFrame
df = pd.json_normalize(json_data, "transactions", max_level=1)

# Return the result to SQL Server
OutputDataSet = df
',
@input_data_1 = N''
@params = N'@json_data NVARCHAR(MAX)',
@json_data = 'Your JSON String Here'
