Sub Main()
        Dim converter = New SynchronizedConverter(New PdfTools())
        Dim pdfExporter = New PdfExporter(converter)

        Dim htmlTable As String = "
        <table>
            <tr>
                <th>Name</th>
                <th>Age</th>
            </tr>
            <tr>
                <td>John</td>
                <td>30</td>
            </tr>
            <tr>
                <td>Alice</td>
                <td>25</td>
            </tr>
        </table>"

        Dim outputPath As String = "table.pdf" ' Set the desired output file path
        pdfExporter.ExportHtmlToPdf(htmlTable, outputPath)

        Console.WriteLine($"PDF exported to: {outputPath}")
    End Sub
