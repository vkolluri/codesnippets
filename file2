 Shared Function GenerateHtmlTable(data As Object) As String
        If data Is Nothing Then
            Return String.Empty
        End If

        Dim html As New StringBuilder()
        html.AppendLine("<table class='table table-bordered table-striped'>")

        If TypeOf data Is JObject Then
            Dim jObject As JObject = DirectCast(data, JObject)

            For Each [property] As JProperty In jObject.Properties()
                html.AppendLine("<tr>")
                html.AppendLine($"<th>{[property].Name}</th>")
                html.AppendLine("<td>")

                Dim childHtml As String = GenerateHtmlTable([property].Value)
                html.AppendLine(childHtml)

                html.AppendLine("</td>")
                html.AppendLine("</tr>")
            Next
        ElseIf TypeOf data Is JArray Then
            Dim jArray As JArray = DirectCast(data, JArray)

            For Each item As JToken In jArray
                html.AppendLine("<tr>")
                html.AppendLine("<th></th>")
                html.AppendLine("<td>")

                Dim childHtml As String = GenerateHtmlTable(item)
                html.AppendLine(childHtml)

                html.AppendLine("</td>")
                html.AppendLine("</tr>")
            Next
        Else
            ' Display simple values
            html.AppendLine($"<td>{data}</td>")
        End If

        html.AppendLine("</table>")
        Return html.ToString()
    End Function
End Class
