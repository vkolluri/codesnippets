Public Sub ExportHtmlToPdf(ByVal html As String, ByVal outputPath As String)
        Dim doc = New HtmlToPdfDocument() With {
            .GlobalSettings = New GlobalSettings() With {
                .PaperSize = PaperKind.A4,
                .Orientation = Orientation.Portrait
            }
        }

        ' Initialize and add ObjectSettings to the Objects collection
        Dim objectSettings = New ObjectSettings() With {
            .PagesCount = True,
            .HtmlContent = html
        }
        doc.Objects.Add(objectSettings)

        Dim pdfBytes As Byte() = _converter.Convert(doc)
        File.WriteAllBytes(outputPath, pdfBytes)
    End Sub
