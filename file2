 Public Function ParseHtmlTable(htmlString As String) As Table
        ' Create a new Table control
        Dim htmlTable As New Table()

        ' Create a TextReader to load HTML string as XML
        Using reader As New StringReader("<root>" & htmlString & "</root>")
            Dim settings As New System.Xml.XmlReaderSettings()
            settings.ConformanceLevel = System.Xml.ConformanceLevel.Fragment
            settings.IgnoreWhitespace = True

            ' Create an XmlReader
            Using xmlReader As System.Xml.XmlReader = System.Xml.XmlReader.Create(reader, settings)
                While xmlReader.Read()
                    If xmlReader.NodeType = System.Xml.XmlNodeType.Element AndAlso xmlReader.Name = "table" Then
                        ' Create a new TableRow for each row in the table
                        Dim tableRow As New TableRow()

                        While xmlReader.Read()
                            If xmlReader.NodeType = System.Xml.XmlNodeType.Element AndAlso
                                (xmlReader.Name = "td" OrElse xmlReader.Name = "th") Then

                                ' Create a new TableCell for each cell in the row
                                Dim tableCell As New TableCell()

                                ' Read the inner text of the cell
                                tableCell.Text = xmlReader.ReadElementContentAsString()

                                ' Add the table cell to the table row
                                tableRow.Cells.Add(tableCell)
                            ElseIf xmlReader.NodeType = System.Xml.XmlNodeType.EndElement AndAlso
                                xmlReader.Name = "tr" Then
                                ' End of the row, add the table row to the table
                                htmlTable.Rows.Add(tableRow)
                                Exit While
                            End If
                        End While
                    End If
                End While
            End Using
        End Using

        Return htmlTable
    End Function
