 Function GenerateHtmlTable(data As Object) As String
        If data Is Nothing Then
            Return String.Empty
        End If

        Dim html As New StringBuilder()
        html.AppendLine("<table class='table table-bordered table-striped'>")

        Dim jToken As JToken = TryCast(data, JToken)

        If jToken IsNot Nothing Then
            If jToken.Type = JTokenType.Object Then
                For Each [property] As JProperty In jToken.Children(Of JProperty)()
                    html.AppendLine("<tr>")
                    html.AppendLine($"<th>{[property].Name}</th>")
                    html.AppendLine("<td>")

                    Dim childHtml As String = GenerateHtmlTable([property].Value)
                    html.AppendLine(childHtml)

                    html.AppendLine("</td>")
                    html.AppendLine("</tr>")
                Next
            ElseIf jToken.Type = JTokenType.Array Then
                For Each item As JToken In jToken
                    html.AppendLine("<tr>")
                    html.AppendLine("<th></th>")
                    html.AppendLine("<td>")

                    Dim childHtml As String = GenerateHtmlTable(item)
                    html.AppendLine(childHtml)

                    html.AppendLine("</td>")
                    html.AppendLine("</tr>")
                Next
            Else
                ' Display simple values
                html.AppendLine($"<td>{jToken}</td>")
            End If
        Else
            ' Display simple values
            html.AppendLine($"<td>{data}</td>")
        End If

        html.AppendLine("</table>")
        Return html.ToString()
    End Function
